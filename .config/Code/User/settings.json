{
  "debug.openDebug": "openOnDebugBreak",
  "diffEditor.ignoreTrimWhitespace": false,
  "editor.bracketPairColorization.enabled": false,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.fontFamily": "'Fira Code Retina', 'monospace', monospace, 'Droid Sans Fallback'",
  "editor.fontLigatures": true,
  "editor.fontSize": 14,
  "editor.formatOnSave": true,
  "editor.linkedEditing": true,
  "editor.minimap.enabled": false,
  "editor.multiCursorModifier": "ctrlCmd",
  "editor.rulers": [80, 100],
  "editor.stickyScroll.enabled": true,
  "editor.wordWrap": "on",
  "explorer.confirmDragAndDrop": false,
  "explorer.confirmDelete": false,
  "extensions.ignoreRecommendations": true,
  "git.confirmSync": false,
  "git.autofetch": true,
  "keyboard.dispatch": "keyCode",
  "terminal.integrated.fontFamily": "Fira Code Retina, Symbols Nerd Font Mono",
  "update.mode": "manual",
  "vim.enableNeovim": true,
  "vim.neovimPath": "/usr/bin/nvim",
  "vim.useSystemClipboard": true,
  "window.menuBarVisibility": "toggle",
  "workbench.colorTheme": "Plastic - deprioritised punctuation",
  "workbench.iconTheme": "material-icon-theme",
  "workbench.settings.enableNaturalLanguageSearch": false,
  "workbench.startupEditor": "none",
  "[dart]": {
    "editor.defaultFormatter": "Dart-Code.dart-code",
    "editor.detectIndentation": false,
    "editor.formatOnType": true,
    "editor.insertSpaces": true,
    "editor.selectionHighlight": false,
    "editor.suggestSelection": "first",
    "editor.suggest.insertMode": "replace",
    "editor.tabCompletion": "onlySnippets",
    "editor.tabSize": 2,
    "editor.wordBasedSuggestions": "off"
  },
  "[clojure]": {
    "editor.defaultFormatter": null
  },
  "[css]": {
    "editor.suggest.insertMode": "replace",
    "editor.tabSize": 2
  },
  "[go]": {
    "editor.defaultFormatter": "golang.go"
  },
  "[html]": {
    "editor.suggest.insertMode": "replace",
    "editor.tabSize": 2
  },
  "[javascript]": {
    "editor.tabSize": 2
  },
  "javascript.updateImportsOnFileMove.enabled": "always",
  "[latex]": {
    "editor.defaultFormatter": null
  },
  "[python]": {
    "editor.defaultFormatter": "ms-python.python",
    "editor.rulers": [80]
  },
  "[rust]": {
    "editor.defaultFormatter": "rust-lang.rust-analyzer"
  },
  "rust-analyzer.checkOnSave": true,
  "rust-analyzer.server.path": "/usr/bin/rust-analyzer",
  "rust-analyzer.files.excludeDirs": ["/target"],
  "[typescript]": {
    "editor.tabSize": 2
  },
  "typescript.updateImportsOnFileMove.enabled": "always",
  "[typescriptreact]": {
    "editor.tabSize": 2
  }
}
